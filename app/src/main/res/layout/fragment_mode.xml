<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/text_mode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="488dp"
        android:layout_height="99dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.012"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_mode"
        app:layout_constraintVertical_bias="0.107">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <Space
                android:layout_width="50dp"
                android:layout_height="wrap_content" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageButton
                android:id="@+id/imageButton1"
                style="@style/Widget.AppCompat.ImageButton"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_centerHorizontal="true"
                android:background="#FA8499BE"
                android:focusable="auto"
                android:scaleType="fitXY"
                android:src="@drawable/ic_mode_lights_on"
                android:tint="#FFC107" />

            <Space
                android:layout_width="50dp"
                android:layout_height="wrap_content" />

            <ImageButton
                android:id="@+id/imageButton2"
                style="@style/Widget.AppCompat.ImageButton"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="#423E3E"
                android:scaleType="fitXY"
                android:src="@drawable/ic_mode_lights_on"
                android:tint="#C87EFF22" />

            <Space
                android:layout_width="50dp"
                android:layout_height="wrap_content" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:background="#779C27B0"
                android:scaleType="fitStart"
                android:src="@drawable/ic_mode_lights_on"
                android:tint="#03A9F4" />
        </TableRow>
    </TableLayout>

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="414dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout"
        app:layout_constraintVertical_bias="0.0">

        <Space
            android:layout_width="35dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Blink"
            android:textAlignment="gravity"
            app:autoSizeTextType="uniform" />

        <Space
            android:layout_width="28dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Blinkalt" />

        <Space
            android:layout_width="20dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Sparkle" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="411dp"
        android:layout_height="86dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow"
        app:layout_constraintVertical_bias="0.0">

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton4"
            style="@style/Widget.AppCompat.ImageButton"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_centerHorizontal="true"
            android:background="#FA8499BE"
            android:focusable="auto"
            android:scaleType="fitXY"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#FF7B07" />

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton5"
            style="@style/Widget.AppCompat.ImageButton"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="#423E3E"
            android:scaleType="fitXY"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#4CAF50" />

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="#779C27B0"
            android:scaleType="fitStart"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#2196F3" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="411dp"
        android:layout_height="51dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="216dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow2"
        app:layout_constraintVertical_bias="0.0">

        <Space
            android:layout_width="35dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Sparkle 2"
            android:textAlignment="gravity"
            app:autoSizeTextType="uniform" />

        <Space
            android:layout_width="28dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Strobo" />

        <Space
            android:layout_width="20dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Cycle Colors" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="503dp"
        android:layout_height="87dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow3"
        app:layout_constraintVertical_bias="0.0">

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton7"
            style="@style/Widget.AppCompat.ImageButton"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_centerHorizontal="true"
            android:background="#FA8499BE"
            android:focusable="auto"
            android:scaleType="fitXY"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#FF4107" />

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton8"
            style="@style/Widget.AppCompat.ImageButton"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="#423E3E"
            android:scaleType="fitXY"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#F8317736" />

        <Space
            android:layout_width="50dp"
            android:layout_height="wrap_content" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="#779C27B0"
            android:scaleType="fitStart"
            android:src="@drawable/ic_mode_lights_on"
            android:tint="#C42C31D6" />
    </TableRow>

    <TableRow
        android:layout_width="491dp"
        android:layout_height="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow4"
        app:layout_constraintVertical_bias="0.0">

        <Space
            android:layout_width="37dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Pixel shift left"
            android:textAlignment="gravity"
            app:autoSizeTextType="uniform" />

        <Space
            android:layout_width="28dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Pixel shift right" />

        <Space
            android:layout_width="24dp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Pixel bounce" />
    </TableRow>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="360dp"
        android:layout_marginBottom="731dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_end="360dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="374dp" />

</androidx.constraintlayout.widget.ConstraintLayout>